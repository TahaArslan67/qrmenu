{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/index",
      "destination": "/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "fbclid"
        }
      ],
      "destination": "/index.html"
    }
  ]
}
