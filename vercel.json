{
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "has": [
        {
          "type": "query",
          "key": "fbclid"
        }
      ],
      "dest": "/static/index.html"
    },
    {
      "src": "/",
      "dest": "/static/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
